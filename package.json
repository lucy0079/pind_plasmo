{
  "name": "pind",
  "displayName": "pind",
  "version": "0.0.1",
  "description": "youtube_location_map",
  "author": "bin",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "change-case": "^5.4.4",
    "plasmo": "0.90.5",
    "pnpm": "^10.14.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "http://localhost:8000/*",
      "https://www.youtube.com/*"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "assets/location.png"
        ],
        "matches": [
          "<all_urls>"
        ]
      }
    ],
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self';"
    },
    "permissions": [
      "storage",
      "notifications"
    ]
  }
}
